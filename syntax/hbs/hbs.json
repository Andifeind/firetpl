{
	"name": "Handelbars",
	"patterns": [
		{
			"name": "tag",
			"match": "(?:<([a-zA-Z][a-zA-Z0-9:_-]*[^>])*>)"
		}, {
			"name": "endtag",
			"match": "(?:<\\/([a-zA-Z][a-zA-Z0-9:_-]+)>)"
		}, {
			"name": "helper",
			"match": "(?:\\{\\{#([a-zA-Z][a-zA-Z0-9_-]*)(?:\\s*([^\\}]*)\\}\\})?)"
		}, {
			"name": "string",
			"match": "(\")"
		}
	],
	"modifer": "gm",
	"scopes": {
		"1": "tag",
		"2": "endtag",
		"3": "helper",
		"4": "expression"
	}
}