section class="test"
	h1 $title
	div class="listing"
		:each $listing : ul
			li
				span class="index" $index
				span class="title" $title
				span class="tags"
					:each $tags : ul class="tags"
						li class="tag" $tag
	div class="comments"
		:if $comments
			:each $comments : ul
				li class="comment"
					span class="user" $user
					span class="comment" $comment
		:else
			div class="noComments" "No comments available yet"
